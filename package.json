{
  "name": "di-onboarding-product-page",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev1": "nodemon src/app.ts",
    "build": "tsc -p .",
    "buildsass": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js assets/css/app.scss dist/assets/app.css",
    "dev": "npm run build && npm run buildsass && npm run buildjs && npm run buildgovuk && npm run buildlocalimages && npm run start",
    "buildjs": "uglifyjs --verbose node_modules/govuk-frontend/govuk/all.js -o dist/assets/govuk-all.js && uglifyjs --verbose assets/javascripts/cookies.js -o dist/assets/cookies.js &&  cp -v -r node_modules/gaap-analytics/build/gaap-analytics.min.js dist/assets/gaap-analytics.min.js",
    "buildgovuk": "cp -v -r node_modules/govuk-frontend/govuk/assets/* ./dist/assets && echo \"Done!\"",
    "buildlocalimages": "cp -v -r assets/images/* ./dist/assets/images"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.42.0",
    "@googleapis/sheets": "^0.3.0",
    "axios": "^0.24.0",
    "gaap-analytics": "^2.2.1",
    "google-auth-library": "^7.10.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "googleapis": "^91.0.0",
    "govuk-frontend": "^3.13.1",
    "node-sass-package-importer": "^5.3.2",
    "nunjucks": "^3.2.3",
    "rfc822-validate": "^1.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.9",
    "@types/nunjucks": "^3.2.0",
    "@types/uuid": "^8.3.3",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "uglify": "^0.1.5"
  }
}
